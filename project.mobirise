        {
          "_name": "team01",
          "_sourceTheme": "carm4",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-section-title": {
              "& when (@showTitleMask)": {
                "span": {
                  "line-height": "1",
                  "color": "@maskTextColor",
                  "padding": "0 6px 1px 5px",
                  "transform": "skew(-14deg)",
                  "background": "@maskColor",
                  "& when (@gradientMask)": {
                    "background": "linear-gradient(@maskColor 0%, @maskColor2 69%, @maskColor3 100%)"
                  },
                  "display": "inline-block",
                  "font-weight": "500",
                  "letter-spacing": "0",
                  "position": "relative"
                }
              }
            },
            ".card": {
              "&:hover": {
                ".card-wrapper": {
                  ".img-wrapper": {
                    ".mbr-section-btn": {
                      "opacity": "1",
                      "transform": "translateY(-50px)",
                      "& when (@cardsAmount = \"3\")": {
                        "transform": "translateY(40px)"
                      },
                      "& when (@cardsAmount = \"4\")": {
                        "transform": "translateY(85px)"
                      }
                    }
                  }
                }
              },
              ".card-wrapper": {
                ".img-wrapper": {
                  "overflow": "hidden",
                  "trasnition": "all 0.3s",
                  "transition": "all 0.3s",
                  "position": "relative",
                  "margin-bottom": "1.5rem",
                  "img": {
                    "width": "100%",
                    "transition": "all 0.6s ease-in-out"
                  },
                  ".mbr-section-btn": {
                    "position": "absolute",
                    "bottom": "150px",
                    "transition": "all 0.2s ease-in-out 0s",
                    "opacity": "0",
                    "transform": "translateY(100px)",
                    "width": "100%",
                    ".btn": {
                      "&.display-4": {
                        "padding": "7px 39px",
                        "letter-spacing": "0"
                      }
                    }
                  }
                },
                ".card-box": {
                  ".card-title": {
                    "color": "#06182d",
                    "font-weight": "700",
                    "margin-bottom": "0",
                    "&.display-7": {
                      "line-height": "1.2"
                    }
                  },
                  ".mbr-section-subtitle": {
                    "color": "@infoColor",
                    "letter-spacing": "0"
                  },
                  ".text-wrap": {
                    "transition": "all 0.3s",
                    "padding": "23px",
                    "bottom": "0",
                    "z-index": "2",
                    "height": "100%",
                    "display": "flex",
                    "flex-direction": "row",
                    "justify-content": "center",
                    "align-items": "flex-end",
                    "width": "100%",
                    "overflow": "hidden",
                    "position": "relative",
                    ".icons-wrap": {
                      ".icon-wrapper": {
                        "background": "@IconsBGColor",
                        "display": "flex",
                        "width": "31px",
                        "height": "31px",
                        "justify-content": "center",
                        "align-items": "center",
                        "border-radius": "50%",
                        "border": "2px solid @iconsLinesColor",
                        "transition": "all 0.3s",
                        "z-index": "4",
                        "cursor": "pointer",
                        "margin": "0 0.2rem",
                        "&:hover": {
                          ".link-icon": {
                            "color": "@iconsHoverColor"
                          }
                        },
                        ".link-icon": {
                          "display": "inline-block",
                          "color": "@IconsColor",
                          "transition": "all 0.3s",
                          "font-size": "14px",
                          "justify-content": "center"
                        }
                      },
                      "&::before": {
                        "height": "2px",
                        "width": "25%",
                        "top": "50%",
                        "right": "70%",
                        "position": "absolute",
                        "display": "inline-block",
